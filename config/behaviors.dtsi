        td_b0:          swich_bluetooth0 {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&bt BT_SEL 0>, <&bt BT_SEL 1>, <&bt BT_SEL 2>, <&bt BT_SEL 3>, <&bt BT_SEL 4>;
                                        };

        td_bcl0:        clear_or_bluetooth0 {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&bt BT_SEL 0>, <&bt BT_CLR>;
                                        };

        td_lara:        left_alt_right_alt {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&kp LALT>, <&kp RALT>;
                                        };
            
        td_lcrc:        left_controll_right_controll {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&kp LCTRL>, <&kp RCTRL>;
                                        };

        td_mo2mo6:        mo2mo6 {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <300>;
                                bindings = <&mo 2>, <&mo 6>;
                                        };

	hrm_c: 	    homerow_mod_custom {
            		compatible = "zmk,behavior-hold-tap";
            	        label = "HOMEROW_MODS";
            		#binding-cells = <2>;
            		tapping-term-ms = <200>;
           		flavor = "tap-preferred";
            	        bindings = <&kp>, <&kp>;
        		        };

        mo_tog: behavior_mo_tog {
                        compatible = "zmk,behavior-hold-tap";
                        #binding-cells = <2>;
                        flavor = "hold-preferred";
                        tapping-term-ms = <200>;
                        bindings = <&mo>, <&kp>;
                                };

        td_d_c:         point_colon {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&mo_tog 7 DOT>, <&kp GREATER_THAN>;
                                        };

        td_c_s:         coma_semicolon {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&kp COMMA>, <&kp LESS_THAN>;
