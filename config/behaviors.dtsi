        td_l_dmo6_ctrl: D_mo6_D_ctrl {
                                compatible = "zmk,behavior-tap-dance";
                                #binding-cells = <0>;
                                tapping-term-ms = <200>;
                                bindings = <&mo_tog 6 D>, <&hrm_c LCTRL D>;
