#include <behaviors.dtsi>
/ {
    macros {
        ws: window_switch {
            compatible = "zmk,behavior-macro";
            label = "ws";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&mo 8 &kp LGUI &kp ESC>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&mo 8 &kp LGUI &kp ESC>;
        };

        la: layer11_lalt_macro {
            compatible = "zmk,behavior-macro";
            label = "la";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&mo 12 &kp LALT>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&mo 12 &kp LALT>;
        };

        ls2: hockn_layer6_lshft_macro {
            compatible = "zmk,behavior-macro";
            label = "ls2";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&mo 4 &kp LSHFT>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&mo 4 &kp LSHFT>;
        };

        lsp: layer11_shiftspace_macro {
            compatible = "zmk,behavior-macro";
            label = "lsp";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LSHFT &kp SPACE>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&kp LSHFT &kp SPACE>;
        };

        mlt: mlesser_than {
            compatible = "zmk,behavior-macro";
            label = "mlt";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LALT &kp KP_N6 &kp KP_N0>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&kp LALT &kp KP_N6 &kp KP_N0>;
        };

        mgt: mgreater_than {
            compatible = "zmk,behavior-macro";
            label = "mgt";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LALT &kp KP_N6 &kp KP_N2>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&kp LALT &kp KP_N6 &kp KP_N2>;
        };

        dd: double_d {
            compatible = "zmk,behavior-macro";
            label = "mgt";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp D &kp D>,
                <&macro_release>,
                <&kp D &kp D>;
        };





        dm: durchmesser {
            compatible = "zmk,behavior-macro";
            label = "dm";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LALT &kp KP_N8 &kp KP_N9 &kp KP_N6 &kp KP_N0>,
                <&macro_pause_for_release>,
                <&macro_release>,
                <&kp LALT &kp KP_N8 &kp KP_N9 &kp KP_N6 &kp KP_N0>;
        };
    };
};
